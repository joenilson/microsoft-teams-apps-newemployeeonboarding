{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.5/MicrosoftTeams.schema.json",
  "manifestVersion": "1.5",
  "version": "1.0.0",
  "id": "1c412988-ba71-40af-8b9b-b5be27fe3a6f",
  "packageName": "com.microsoft.teams.newemployeeonboarding",
  "developer": {
    "name": "<company name>",
    "websiteUrl": "<website Url>",
    "privacyUrl": "<company privacy Url>",
    "termsOfUseUrl": "<company terms of use Url>"
  },
  "localizationInfo": {
    "defaultLanguageTag": "en",
    "additionalLanguages": [
      {
        "languageTag": "en-US",
        "file": "en-US.json"
      },
      {
        "languageTag": "es-ES",
        "file": "es-ES.json"
      }
    ]
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "Incorporación de nuevos empleados",
    "full": "Incorporación de nuevos empleados"
  },
  "description": {
    "short": "La nueva aplicación de incorporación de empleados simplificará la incorporación de empleados",
    "full": "La aplicación de incorporación de nuevos empleados simplificará la incorporación de nuevos empleados utilizando MS Teams y SharePoint Online. El bot NEO se creará para satisfacer los siguientes requisitos."
  },
  "accentColor": "#127E1D",
  "bots": [
    {
      "botId": "<<botId>>",
      "scopes": [
        "personal",
        "team"
      ],
      "commandLists": [
        {
          "scopes": [
            "personal"
          ],
          "commands": [
            {
              "title": "Lista de verificación de nuevos empleados",
              "description": "Ver tu ruta de aprendizaje"
            },
            {
              "title": "Compartir comentarios",
              "description": "Envíe una idea o comentario"
            },
            {
              "title": "Revisar las introducciones",
              "description": "Revisar las introducciones de los empleados"
            },
            {
              "title": "Ayuda",
              "description": "Hacer un recorrido"
            }
          ]
        }
      ],
      "supportsFiles": false,
      "isNotificationOnly": false
    }
  ],
  "staticTabs": [
    {
      "entityId": "Ruta",
      "name": "Lista de verificación de nuevos empleados",
      "contentUrl": "<<SharePointNewHireCheckListUrl>>",
      "scopes": [
        "personal"
      ]
    }
  ],
  "configurableTabs": [
    {
      "configurationUrl": "<<azurefdurl>>/config-tab",
      "canUpdateConfiguration": true,
      "scopes": [
        "team"
      ]
    }
  ],
  "validDomains": [
    "<<appDomain>>"
  ],
  "webApplicationInfo": {
    "id": "<<botId>>",
    "resource": "<ApplicationIdURI>"
  }
}